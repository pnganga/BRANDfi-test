<body class="animsition">
    <div class="page-wrapper">
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block" style="color: white;">
            <div class="logo">
                <a href="#">
                    <img src="/img/BRANDfii.png"  class="img-responsive img-cir" alt="BrandFi" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class=" has-sub ">
                            <a class="js-arrow" style="text-decoration: none; color: white;" href="/client/dashboard" >
                                <i class="fas fa-tachometer-alt"></i><span >Dashboard</span></a>
                        </li>
                        <li class="has-sub">
                            <a class="js-arrow" style="text-decoration: none; color: white;" href="/client/allusers">
                                <i class="fas fa-user"></i>Users</a>
                        </li>
                        <li class="active has-sub">
                            <a class="js-arrow" style="text-decoration: none; color: white;" href="/client/customize/mobile">
                                <i class="fas fa-sliders-h"></i>Splash Pages</a>
                        </li>
                        <li>
                            <a href="#" style="text-decoration: none; color: white;">
                                <i class="fas fa-table"></i>Analytics</a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content row" style="padding-left: 200px;">
                <br>
                <div class="row">
                    <div class="col-sm-4"></div>
                    <div class="col-sm-4"></div>
                    <div class="col-sm-4">
                        <button class="btn btn-lg btn-primary">Send SMS to all Users</button>
                    </div>
                </div>
                <br>
                <table class="table">
                    <thead>
                        <tr>
                            <th>id</th>
                            <th>Username</th>
                            <th>Mobile Number</th>
                            <th>Mac Address</th>
                            <th>SSID</th>
                        </tr>
                    </thead>
                    {{#users}}
                    <tbody>
                        <tr>
                            <td>{{id}}</td>
                            <td>{{username}}</td>
                            <td>{{mobileNumber}}</td>
                            <td>{{macAddress}}</td>
                            <td>{{ssid}}</td>
                            <td><button class="btn btn-inverse btn-primary">send SMS</button></td>
                        </tr>
                    </tbody>
                    {{/users }}
                </table>
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>
    </div>
    <!-- Jquery JS-->
    <script src="/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="/vendor/slick/slick.min.js">
    </script>
    <script src="/vendor/wow/wow.min.js"></script>
    <script src="/vendor/animsition/animsition.min.js"></script>
    <script src="/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="/vendor/select2/select2.min.js">
    </script>
    <!-- Main JS-->
    <script src="/js/main.js"></script>
    <!-- end document-->